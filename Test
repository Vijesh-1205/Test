var a=10
var b=5
function Evaluate(a,b){
    console.log(a+b)
    console.log(a-b)
    console.log(a*b)
    console.log(a/b)
}
Evaluate(a,b)


console.log("question 2")
var arr=[1,2,3,4];
var even=[];
for(var i=0;i<arr.length;i++){
    if (arr[i]%2==0){
        even.push(arr[i])
    }
}
console.log(even);



console.log("question 3")
var arr=[1,2,3,4];
var multiple=[];
for(var i=0;i<arr.length;i++){
    var mul=arr[i]*2
    multiple.push(mul)
}
console.log(multiple)



